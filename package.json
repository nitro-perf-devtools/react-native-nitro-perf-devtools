{
  "name": "nitroperf-monorepo",
  "private": true,
  "workspaces": [
    "packages/*",
    "example",
    "docs"
  ],
  "scripts": {
    "codegen": "cd packages/core && npx nitrogen",
    "typecheck": "tsc --noEmit",
    "lint": "eslint packages/*/src/**/*.{ts,tsx}",
    "build": "cd packages/core && bob build",
    "example:start": "cd example && npm start",
    "example:ios": "cd example && npm run ios",
    "example:android": "cd example && npm run android",
    "example:prebuild": "cd example && npm run prebuild",
    "docs:dev": "cd docs && npm start",
    "docs:build": "cd docs && npm run build",
    "docs:deploy": "cd docs && fly deploy"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.73.0",
    "eslint": "^8.56.0",
    "react-native-builder-bob": "^0.23.0"
  }
}
