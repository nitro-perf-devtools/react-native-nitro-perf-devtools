{
  "name": "react-native-nitro-perf-monorepo",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "codegen": "cd packages/react-native-nitro-perf && npx nitrogen",
    "typecheck": "tsc --noEmit",
    "lint": "eslint packages/*/src/**/*.{ts,tsx}",
    "build": "cd packages/react-native-nitro-perf && bob build",
    "example:ios": "cd ios && pod install && cd .. && npx react-native run-ios",
    "example:android": "npx react-native run-android"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.73.0",
    "eslint": "^8.56.0",
    "react-native-builder-bob": "^0.23.0"
  }
}
