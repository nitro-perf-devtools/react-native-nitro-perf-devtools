{
  "expo": {
    "name": "Nitro Perf Example",
    "slug": "nitro-perf-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "plugins": [
      [
        "@nitroperf/core",
        {
          "releaseLevel": "canary",
          "enableTurboInterop": true
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f0f23"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nitroperf.example",
      "infoPlist": {
        "ReactNativeReleaseLevel": "canary"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0f0f23"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.nitroperf.example"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
